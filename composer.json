{
  "name": "ged/api-laravel",
  "version": "2.4.5",
  "homepage": "https://sig.ged.api.br",
  "support": {
    "email": "suporte@ged.api.br",
    "docs": "https://ged.api.br/docs"
  },
  "description": "Laravel Package para integração com GED API — Assinatura Digital PAdES ICP-Brasil em 3 Fases",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "assinatura-digital",
    "pades",
    "icp-brasil",
    "certificado-digital",
    "pkcs7",
    "cms",
    "pdf-signature"
  ],
  "authors": [
    {
      "name": "Equipe GED API",
      "email": "suporte@ged.api.br",
      "homepage": "https://ged.api.br"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "phpseclib/phpseclib": "^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0|^8.0|^9.0",
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Ged\\ApiLaravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ged\\ApiLaravel\\GedApiServiceProvider"
      ],
      "aliases": {
        "GedApi": "Ged\\ApiLaravel\\Facades\\GedApi"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}

